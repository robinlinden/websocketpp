## logger unit tests
##

Import('env')
Import('boostlibs')
Import('platform_libs')

env = env.Clone ()

ALL_LIBS = boostlibs(['unit_test_framework','system'],env) + [platform_libs]

objs = env.Object('logger_basic.o', ["basic.cpp"], LIBS = ALL_LIBS)
prgs = env.Program('logger_basic', ["logger_basic.o"], LIBS = ALL_LIBS)

Return('prgs')
